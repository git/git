#: add-patch.c
#, c-format
msgid "Stage mode change [y,n,q,a,d%s,?]? "
msgstr "暫存模式更動 [y,n,q,a,d%s,?]? "

#: add-patch.c
#, c-format
msgid "Stage deletion [y,n,q,a,d%s,?]? "
msgstr "暫存刪除動作 [y,n,q,a,d%s,?]? "

