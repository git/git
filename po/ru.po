msgstr "отправить метки (нельзя использовать вместе с --all или --mirror)"

#: builtin/push.c:547 builtin/send-pack.c:164
msgid "force updates"
msgstr "принудительное обновление"

#: builtin/push.c:549 builtin/send-pack.c:175
